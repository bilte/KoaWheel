{
  "name": "koawheel",
  "app": "bin/www",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node $npm_package_app",
    "debug": "./node_modules/.bin/nodemon $npm_package_app",
    "stop": "pkill -9 node",
    "test": "node $npm_package_app --test",
    "pm2": "pm2 start ecosystem.config.js --env dev",
    "prod": "pm2 start ecosystem.config.js --env production",
    "resetDB": "node bin/database",
    "doc": "apidoc -i src/controllers -o doc",
    "deploy": "node bin/deploy.js"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "debug": "^2.6.3",
    "glob": "^7.1.3",
    "ioredis": "^4.6.3",
    "koa": "^2.7.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "koa2-cors": "^2.0.6",
    "log4js": "^3.0.6",
    "mysql2": "^1.6.5",
    "node-ssh": "^5.1.2",
    "pug": "^2.0.0-rc.1",
    "sequelize": "^4.43.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
